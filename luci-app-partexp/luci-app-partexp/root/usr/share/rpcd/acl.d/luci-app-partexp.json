{
    "luci-app-partexp": {
        "description": "Grant UCI access for luci-app-partexp",
	"read": {
            "file": {
                "/etc/init.d/partexp": [ "exec" ],
                "/var/partexp": ["read"],
                "/etc/partexppath": ["read"],
                "/etc/partexp/": ["read"]
            },
            "uci": [ "partexp" ]
	},
	"write": {
            "file": {
                "/var/partexp": ["write"],
                "/etc/partexppath": ["write"],
                "/etc/partexp/": ["write"]
		
            },
            "uci": [ "partexp" ]
        }
    }
}
