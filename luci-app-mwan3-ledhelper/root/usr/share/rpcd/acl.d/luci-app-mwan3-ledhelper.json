{
	"luci-app-mwan3-ledhelper": {
		"description": "Grant access to MWAN3 LED configuration",
		"read": {
			"file": {
				"/etc/init.d/mwan3": [ "exec" ],
				"/etc/config/mwan3": [ "read" ]
			},
			"cgi-io": [ "exec" ],
			"ubus": {
				"file": [ "exec" ],
				"uci": [ "changes", "get" ]
			},
			"uci": [ "mwan3_led" ]
		},
		"write": {
			"cgi-io": [ "exec" ],
			"ubus": {
				"file": [ "exec" ],
				"uci": [ "add", "apply", "confirm", "delete", "order", "rename", "set" ]
			},
			"uci": [ "mwan3_led" ]
		}
	}
}